// Theme Color customizer Right Modal
.customizer-toggle(data-toggle='modal' data-target='#ThemeColorCustomizer')
  i.spin.icon.mt-minus-2(data-feather='settings')
#ThemeColorCustomizer.modal.right.color-customizer-modal.fade
  .modal-dialog(role='document')
    .modal-content
      .modal-header
        button.close(type='button' data-dismiss='modal' aria-label='Close')
          span(aria-hidden='true') &times;
        h4#myModalLabel2.modal-title Theme Color Customizer
      .modal-body
        // Left SideMenu Color Switcher
        .color-content
          h5 Left SideMenu Color
          p.mb-2 Change SideMenu background color
          ul.customize-sidemenu
            li#BGPrimary.bg_primary
            li#BGPurpleIndigo.purple_indigo
            li#BGPink.bg_pink
            li#BGnNightBlue.bg_night_blue
            li#BGIndigo.bg_indigo
            li#BGSuccess.bg_success
            li#BGSecondary.bg_secondary
            li#BGPurple.bg_purple
            li#BGGray.bg_gray
            li#BGDanger.bg_danger
            li#BGGrayBlue.bg_gray_blue
            li#BGGreen.bg_green
            li#BGWarning.bg_warning
            li#BGDeepPurple.bg_deep_purple
        // Left SideMenu Folded Menu
        .color-content
          h5 Folded Menu
          p Toggle Folded Menu
          .side-menu-switch
            label.switch
              input.light(type='checkbox')
              span.slider.round.folded-menu
        // Card Shadow Show & hide
        .color-content
          h5 Card Shadow
          p Show &amp; Hide Card Shadow
          .side-menu-switch
            label.switch
              input.light(type='checkbox')
              span.slider.round.card-shadow
